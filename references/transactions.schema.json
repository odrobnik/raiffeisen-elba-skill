{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "ELBA Transactions Output",
  "type": "array",
  "items": {
    "type": "object",
    "required": ["id", "buchungstag", "neuanlage", "betrag", "iban"],
    "properties": {
      "id": {"type": "string"},
      "buchungstag": {"type": "string", "format": "date"},
      "neuanlage": {"type": "string"},
      "iban": {"type": "string"},
      "valuta": {"type": "string", "format": "date"},
      "betrag": {
        "type": "object",
        "required": ["amount", "currencyCode"],
        "properties": {
          "amount": {"type": "number"},
          "currency": {"type": "string"},
          "currencyCode": {"type": "string"}
        },
        "additionalProperties": true
      }
    },
    "additionalProperties": true
  }
}
